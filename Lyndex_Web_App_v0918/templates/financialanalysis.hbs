            </div>
            <div class="form-group col-md-3">
                <input type="number" class="form-control" name="netMargin2017" id="netMargin2017" value="{{content.financialratio.2017.netmargin}}">
            </div>
            <div class="form-group col-md-3">
                <input type="number" class="form-control" name="netMargin2016" id="netMargin2016" value="{{content.financialratio.2016.netmargin}}">
            </div>
            <div class="form-group col-md-3">
                <input type="number" class="form-control" name="netMargin2015" id="netMargin2015" value="{{content.financialratio.2015.netmargin}}">
            </div>
        </div>

        <hr>

        <button type="submit" class="btn btn-primary">Update</button>
        <a href="accessment.html" role="button" class="btn btn-success">Start Over</a>
